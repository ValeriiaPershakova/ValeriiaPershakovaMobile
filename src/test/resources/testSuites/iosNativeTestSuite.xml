<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Native iOS tests Suite">
    <test name="Native iOS tests">
        <groups>
            <define name="ios native">
                <include name="ios"/>
                <include name="native"/>
            </define>
            <run>
                <include name="ios native"/>
            </run>
        </groups>
        <classes>
            <class name="scenarios.nativetests.NativeSimpleTest"/>
        </classes>
    </test>
</suite>